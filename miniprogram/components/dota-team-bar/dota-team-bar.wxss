/* components/dota-team-bar/dota-team-bar.wxss */
.progress {
    display: flex;
    justify-content: center;
    flex-direction: column;
    height: 100%;
}

.progress-text {
    color: #666;
    font-size: 12px;
    text-align: left;
    padding-bottom: 3px;
}

.progress-bar {
    width: 100%;
    height: 4px;
    border-radius: 2px;
    background: #e8e8e8;
}

.progress-fill {
    height: 100%;
    border-radius: 2px;
    background: #777777;
}