<!--pages/upgrade-log/upgrade-log.wxml-->
<van-tabs color="#000000" title-inactive-color="#787d7e" sticky="true" bind:change="changeEvent">
    <van-tab title="更新日志" name="news_update">
        <log-item bindclick="goNewsDetail" logList="{{newList}}" isFinshed="{{isFinshed}}"></log-item>
    </van-tab>
    <van-tab title="公告" name="announcement">
        <log-item bindclick="goNewsDetail" logList="{{newList}}" isFinshed="{{isFinshed}}"></log-item>
    </van-tab>
</van-tabs>